{
    "coc.preferences.formatOnSaveFiletypes": ["python", "cmake", "lua", "c", "cpp", "html", "typescript", "css", "json"],
    "coc.preferences.formatOnType": true,
    "coc.preferences.useQuickfixForLocations": true,
    "codeActionOnSave.enable": false,
    "prettier.enable": true,
    "suggest.noselect": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": true,
    "languageserver": {
    },
    "python.formatting.provider": "black",
    "inlayHint.enable": true,
    "inlayHint.display": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrace": true
}
